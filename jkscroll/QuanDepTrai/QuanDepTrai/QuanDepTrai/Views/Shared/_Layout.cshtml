<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Skroll</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Scripts.Render("~/bundles/jquery")
    <script src="../../Scripts/skrollr.js" type="text/javascript"></script>
    <link href="../../Content/fixed-positioning.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/scroll.css" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Content/css")
</head>
<body>
    <div id="bigboy" data-0="background:rgb(255, 255, 255)" data-500="background:rgb(52, 152, 219)" data-10000="background:rgb(65, 189, 76)">
