            <div class="float-left">
                <p class="site-title">@Html.ActionLink("Quân Kul Đẹp Trai", "Index", "Home")</p>
            </div>
            <div class="float-right">
                <nav>
                        <ul id="menu">
                            <li><a href="http://www.albinotonnina.com">Demo 1</a></li>
                            <li> <a href="http://20141123.lulu-and-me.com"> Demo 2 </li>
        
                        </ul>
                    </nav>
            </div>
        </div>
        <div id="body">
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
    </div>
</body>
</html>
